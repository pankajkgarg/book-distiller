{
  "name": "vite-react-tailwind",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.16.x"
  },
  "packageManager": "pnpm@10.7.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "sharp"
    ]
  },
  "scripts": {
    "dev": "vite --port 3000",
    "start": "vite --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "generate-pwa-assets": "pwa-assets-generator",
    "turbo": "pnpm dlx turbo@2.5.0",
    "type:check": "tsc",
    "test": "vitest run",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --coverage run",
    "biome:check": "biome check",
    "biome:fix": "pnpm biome:check --write",
    "biome:ci": "biome ci",
    "check:turbo": "pnpm turbo biome:check type:check test",
    "release": "cross-env HUSKY=0 commit-and-tag-version",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-router": "^1.127.3",
    "@tanstack/react-router-devtools": "^1.127.3",
    "@tanstack/router-plugin": "^1.127.5",
    "@theme-toggles/react": "^4.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^4.0.11",
    "jszip": "^3.10.1",
    "lucide-react": "^0.539.0",
    "pdfjs-dist": "^5.4.54",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@julr/vite-plugin-validate-env": "^2.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vitejs/plugin-react-oxc": "^0.2.3",
    "commit-and-tag-version": "^12.5.1",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^16.1.2",
    "typescript": "^5.7.2",
    "vite": "npm:rolldown-vite@7.0.4",
    "vite-plugin-checker": "^0.10.0",
    "vite-plugin-pwa": "^1.0.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "web-vitals": "^5.0.3"
  }
}
